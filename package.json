    "test:headed": "playwright test -c tests/playwright.config.js --headed",
    "test:debug": "playwright test -c tests/playwright.config.js --debug",
    "test:mobile": "playwright test -c tests/playwright.config.js --grep 'Budget Android|Samsung Galaxy|iPhone'",
    "test:desktop": "playwright test -c tests/playwright.config.js --grep 'Desktop'",
    "test:responsive": "playwright test tests/responsive-usability.spec.js -c tests/playwright.config.js",
    "test:performance": "playwright test tests/performance.spec.js -c tests/playwright.config.js",
    "test:tracking": "playwright test tests/tracking.spec.js -c tests/playwright.config.js",
    "test:report": "playwright show-report playwright-report"
  },
  "dependencies": {
    "@fullpage/react-fullpage": "^0.1.28",
    "@reduxjs/toolkit": "^1.8.6",
